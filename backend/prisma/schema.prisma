generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  email       String   @unique
  name        String?
  ideology    String?
  personality String?
  ethnicity   String?
  createdAt   DateTime @default(now())
}
